<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:popups="clr-namespace:BashkirTheatre14.ViewModel.Popups"
                    xmlns:popups1="clr-namespace:BashkirTheatre14.View.Popups">
    <DataTemplate DataType="{x:Type popups:PasswordPopupViewModel}">
        <popups1:PasswordPopupView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type popups:QuizSelectionPopupViewModel}">
        <popups1:QuizSelectionView DataContext="{Binding}"/>
    </DataTemplate>
</ResourceDictionary>