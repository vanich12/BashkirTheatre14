<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:popups="clr-namespace:BashkirTheatre14.ViewModel.Popups"
                    xmlns:popups1="clr-namespace:BashkirTheatre14.View.Popups"
                    xmlns:map="clr-namespace:BashkirTheatre14.View.Controls.Map"
                    xmlns:map1="clr-namespace:BashkirTheatre14.ViewModel.Controls.Map">
    <DataTemplate DataType="{x:Type popups:PasswordPopupViewModel}">
        <popups1:PasswordPopupView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type popups:QuizSelectionPopupViewModel}">
        <popups1:QuizSelectionView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type popups:MapObjectDetailsPopupViewModel}">
        <popups1:MapObjectsDetailsPopupView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type popups:MapSearchPopupViewModel}">
        <popups1:SearchPopupView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type map1:SearchMapObjectPopupViewModel}">
        <map:MapObjectSearchItemView
            Margin="0 0 170 200"/>
    </DataTemplate>
</ResourceDictionary>