<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:pages="clr-namespace:BashkirTheatre14.ViewModel.Pages"
                    xmlns:pages1="clr-namespace:BashkirTheatre14.View.Pages"
                    xmlns:controls="clr-namespace:BashkirTheatre14.ViewModel.Controls">
    <DataTemplate DataType="{x:Type pages:MainPageViewModel}">
        <pages1:PageContainer DataContext="{Binding}">
            <pages1:MainPage/>
        </pages1:PageContainer>
    </DataTemplate>

    <DataTemplate DataType="{x:Type pages:ChroniclesPageViewModel}">
        <pages1:PageContainer DataContext="{Binding}">
            <pages1:ChroniclePage/>
        </pages1:PageContainer>
    </DataTemplate>
</ResourceDictionary>